<AnchorPane xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.vt_labs_1.controllers.MenuController">
   <children>
      <Pane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0">
         <children>
            <ChoiceBox fx:id="commandChoice" accessibleText="add" layoutX="58.0" layoutY="61.0" prefWidth="150.0" />
            <AmbientLight color="CHARTREUSE" lightOn="true">
               <effect>
                  <DropShadow />
               </effect>
            </AmbientLight>
            <Button fx:id="executeButton" layoutX="256.0" layoutY="147.0" mnemonicParsing="false" onAction="#run" text="%ExecuteButton" />
            <Label fx:id="chooseCommand" layoutX="64.0" layoutY="33.0" text="%ChooseCommandRequestLabel" />
            <Button fx:id="exitButton" layoutX="485.0" layoutY="339.0" mnemonicParsing="false" onAction="#exit" text="%ExitButton" />
            <TextField fx:id="argument" layoutX="294.0" layoutY="61.0" prefHeight="24.0" prefWidth="135.0" promptText="%ValueLabel" />
            <Label fx:id="enterArgument" layoutX="297.0" layoutY="38.0" text="%EnterArgumentRequestLabel" />
            <TextArea fx:id="result" editable="false" layoutX="33.0" layoutY="231.0" prefHeight="137.0" prefWidth="298.0" text=" " />
            <Label fx:id="resultLabel" layoutX="33.0" layoutY="192.0" text="%ResultLabel" />
            <Button fx:id="tableButton" layoutX="478.0" layoutY="231.0" mnemonicParsing="false" onAction="#openTable" text="%TableButton" />
